{
  "entities": {
    "team": {
      "singular": "team",
      "plural": "teams",
      "id_prefix": "team",
      "required_params": ["name", "company"],
      "default_values": {
        "company": "TestCompany"
      },
      "response_fields": ["id", "name", "company"],
      "verify_fields": ["company", "vault_encrypted"]
    },
    "machine": {
      "singular": "machine",
      "plural": "machines",
      "id_prefix": "machine",
      "required_params": ["name", "team", "bridge"],
      "default_values": {
        "team": "TestTeam",
        "bridge": "TestBridge"
      },
      "response_fields": ["id", "name", "team", "bridge"],
      "verify_fields": ["team", "bridge", "vault_encrypted"]
    },
    "region": {
      "singular": "region",
      "plural": "regions",
      "id_prefix": "region",
      "required_params": ["name", "company"],
      "default_values": {
        "company": "TestCompany",
        "location": "us-east-1"
      },
      "response_fields": ["id", "name", "company", "location"],
      "verify_fields": ["company", "location"]
    },
    "bridge": {
      "singular": "bridge",
      "plural": "bridges",
      "id_prefix": "bridge",
      "required_params": ["name", "region"],
      "default_values": {
        "region": "TestRegion"
      },
      "response_fields": ["id", "name", "region"],
      "verify_fields": ["region"]
    },
    "repository": {
      "singular": "repository",
      "plural": "repositories",
      "id_prefix": "repo",
      "required_params": ["name", "team", "machine"],
      "default_values": {
        "team": "TestTeam",
        "machine": "TestMachine",
        "repo_type": "git"
      },
      "response_fields": ["id", "name", "team", "machine", "repo_type"],
      "verify_fields": ["team", "machine", "repo_type"]
    },
    "user": {
      "singular": "user",
      "plural": "users",
      "id_prefix": "user",
      "required_params": ["name", "company", "email"],
      "default_values": {
        "company": "TestCompany",
        "role": "User"
      },
      "response_fields": ["id", "name", "company", "email", "role"],
      "verify_fields": ["company", "email", "role"]
    },
    "company": {
      "singular": "company",
      "plural": "companies",
      "id_prefix": "company",
      "required_params": ["name"],
      "default_values": {},
      "response_fields": ["id", "name"],
      "verify_fields": []
    },
    "storage": {
      "singular": "storage",
      "plural": "storages",
      "id_prefix": "storage",
      "required_params": ["name", "team"],
      "default_values": {
        "team": "TestTeam",
        "storage_type": "s3"
      },
      "response_fields": ["id", "name", "team", "storage_type"],
      "verify_fields": ["team", "storage_type"]
    },
    "schedule": {
      "singular": "schedule",
      "plural": "schedules",
      "id_prefix": "schedule",
      "required_params": ["name", "team"],
      "default_values": {
        "team": "TestTeam",
        "cron": "0 0 * * *",
        "enabled": true,
        "function": "backup"
      },
      "response_fields": ["id", "name", "team", "cron", "enabled", "function"],
      "verify_fields": ["team", "cron", "enabled", "function"]
    }
  },
  "command_patterns": {
    "create": {
      "pattern": "create {entity}",
      "response_type": "create",
      "success_field": "success"
    },
    "inspect": {
      "pattern": "inspect {entity} {name}",
      "response_type": "get",
      "success_field": "success"
    },
    "get": {
      "pattern": "get {entity} {name}",
      "response_type": "get",
      "success_field": "success"
    },
    "list": {
      "pattern": "list {entity}",
      "response_type": "list",
      "success_field": "success"
    },
    "update": {
      "pattern": "update {entity} {name}",
      "response_type": "update",
      "success_field": "success"
    },
    "rm": {
      "pattern": "rm {entity} {name}",
      "response_type": "delete",
      "success_field": "success"
    },
    "delete": {
      "pattern": "delete {entity} {name}",
      "response_type": "delete",
      "success_field": "success"
    }
  },
  "special_commands": {
    "queue": {
      "create": {
        "response": {
          "success": true,
          "taskId": "task-{uuid}",
          "status": "PENDING"
        }
      },
      "status": {
        "response": {
          "success": true,
          "taskId": "{taskId}",
          "status": "COMPLETED",
          "result": "Success"
        }
      }
    },
    "login": {
      "response": {
        "success": true,
        "token": "test-token-{uuid}",
        "nextRequestCredential": "next-token-{uuid}"
      }
    }
  },
  "error_responses": {
    "not_found": {
      "success": false,
      "error": "{entity} '{name}' not found"
    },
    "invalid_name": {
      "success": false,
      "error": "Invalid name '{name}'. Use only alphanumeric, dash, and underscore."
    },
    "missing_required": {
      "success": false,
      "error": "Missing required field: {field}"
    },
    "unauthorized": {
      "success": false,
      "error": "Unauthorized: Invalid or expired token"
    }
  }
}